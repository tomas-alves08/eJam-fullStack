    "test": "jest --watchAll",
    "lint": "eslint --ext .js,.jsx .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [
    "fullstack"
  ],
  "babel": {
    "presets": [
      "@babel/preset-typescript",
      "@babel/preset-env",
      "@babel/preset-react"
