  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-regular-svg-icons": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "bootstrap": "^5.2.2",
    "express": "^4.17.2",
    "firebase": "^9.12.1",
    "knex": "^2.1.0",
    "path": "^0.12.7",
    "path-browserify": "^1.0.1",
    "react-bootstrap": "^2.5.0",
    "react-redux": "^8.0.4",
    "react-router": "^6.4.2",
